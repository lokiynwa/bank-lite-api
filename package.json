{
  "name": "bank-lite-api",
  "version": "1.0.0",
  "description": "Demo Node.js/TypeScript API with OpenAPI, JWT auth, versioning, and contract tests",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.js",
    "contract:test": "dredd ./openapi.yaml http://127.0.0.1:3000 --server=\"npm run dev\" --server-wait=3 --header=\"Authorization: Bearer $TEST_JWT\""
  },
  "keywords": [
    "api",
    "typescript",
    "fastify",
    "openapi",
    "jwt",
    "contract-testing"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "fastify": "^5.6.0",
    "fastify-swagger-ui": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.11.0",
    "pino-pretty": "^13.1.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "dredd": "^14.1.0",
    "eslint": "^9.36.0",
    "js-yaml": "^4.1.0",
    "openapi-response-validator": "^12.1.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
